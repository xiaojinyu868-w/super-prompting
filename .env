# SPO+ 环境变量配置

# 服务器配置
PORT=3001
NODE_ENV=development

# 默认API配置
DEFAULT_API_BASE_URL=https://api.siliconflow.cn
# 默认API密钥（实际使用时应由用户提供）
DEFAULT_API_KEY=

# 默认模型配置
DEFAULT_OPTIMIZER_MODEL=Qwen/QwQ-32B
DEFAULT_EXECUTOR_MODEL=deepseek-ai/DeepSeek-R1-Distill-Qwen-32B
DEFAULT_EVALUATOR_MODEL=Pro/deepseek-ai/DeepSeek-V3
DEFAULT_ANALYZER_MODEL=Pro/deepseek-ai/DeepSeek-R1

# 流式输出配置
ENABLE_STREAMING=true
STREAM_CHUNK_SIZE=10 